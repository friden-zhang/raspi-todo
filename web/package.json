{
  "name": "raspi-todo-web",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview --port 5173",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,html,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,html,css,scss}\"",
    "lint": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "prepare": "cd .. && husky install || echo 'Husky not available'"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.7.0",
    "prettier": "^3.6.2",
    "typescript": "^5.6.2",
    "vite": "^5.4.2"
  }
}
